{
    "manifest_version": 2,
    "name": "ngExt",
    "version": "1.0",
    "browser_action": {
    "default_popup": "index.html"
    },
    "permissions": ["http://*.premierleague.com/", "https://*.premierleague.com/"],    
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources": [
    "assets/css/*",
    "assets/js/*",
    "assets/fonts/*"
    ]
   }